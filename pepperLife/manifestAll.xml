<?xml version="1.0" encoding="UTF-8"?>
<package uuid="pepperlife" version="0.6.0">
    <names>
        <name lang="en_US">PepperLife GPT - alpha</name>
        <name lang="fr_FR">PepperLife GPT - alpha</name>
    </names>
    <descriptions>
        <description lang="en_US">PepperLife robot settings and gpt ( http://IPROBOT:8080) </description>
        <description lang="fr_FR">PepperLife robot settings et gpt ( http://IPROBOT:8080) </description>
    </descriptions>
    <services>
        <service autorun="true" name="pepperlife" execStart="python2 bin/lanceur.py" />
