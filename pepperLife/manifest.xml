<?xml version="1.0" encoding="UTF-8"?>
<package uuid="pepperlife" version="1.6.1" author="Anthony Gallot">
    <names>
        <name lang="en_US">PepperLife GPT</name>
        <name lang="fr_FR">PepperLife GPT</name>
    </names>
    <descriptions>
        <description lang="en_US">PepperLife robot settings and gpt ( http://IPROBOT:8080 )</description>
        <description lang="fr_FR">PepperLife robot settings et gpt ( http://IPROBOT:8080 )</description>
    </descriptions>
    <services>
        <service autorun="true" name="pepperlife" execStart="python2 bin/lanceur.py"/>
    </services>
    <executableFiles>
        <file path="python"/>
    </executableFiles>
    <requirements>
        <naoqiRequirement maxVersion="2.9" minVersion="2.9"/>
        <robotRequirement model="JULIETTE"/>
        <robotRequirement model="NAO"/>
    </requirements>
    <category>Danses</category>
</package>
